{
  "name": "newapp",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js --open",
    "build": "vue-cli-service build --name newapp --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "doc:build": "npx vuedoc.md --section API --output ./README.md ./src/components/Chat.vue",
    "prepublishOnly": "npm run lint && npm run build && npm run styleguide:build && npm run doc:build",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "main": "dist/newapp.umd.js",
  "module": "dist/newapp.common.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "dependencies": {
    "core-js": "^3.4.3",
    "socket.io": "^2.3.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vuedoc/md": "^1.5.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-component": "^1.12.2",
    "vue-styleguidist": "^1.7.13",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "vue",
    "component"
  ]
}
